!slot <android>;

!start <callStart>;
<callStart>:
[<callPre>][<callTo>]<contact><callPhone>
|[<callPre>]<callPhone>[<callTo>]<contact>
|[<callPre>]<callPhone>

|<movePre><move>

|<cmdPre><cmd>

|<animation>

|<good>

|<general>

|<bad>

|<price>

|<goodbye>

|<buy>

|<touch>

|<small>

|<big>

